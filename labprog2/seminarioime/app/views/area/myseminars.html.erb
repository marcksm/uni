<h1> My Seminars </h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Group</th>
      <th>Description</th>
      <th>Place</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @seminars.each do |seminar| %>
      <tr>
        <td><%= seminar.name %></td>
        <td><%= seminar.group.name %></td>
        <td><%= seminar.description %></td>
        <td><%= seminar.place %></td>
        <td><%= seminar.date %></td>
        <td><%= link_to 'Show', seminar %></td>
      </tr>
    <% end %>
  </tbody>
</table>
